@model List<SalesOrder>

@{
    Layout = "_C286";
}
<h2>@ViewBag.Title</h2>
<div style="background-color: white;">
    <table class="table table-bordered table-condensed table-hover table-striped">
        <tr>
            <th>Product Id</th>
            <th>Price</th>
            <th>Qty</th>
        </tr>
        @foreach (SalesOrder order in Model!)
        {
            <tr>
                <td>@order.ProductId</td>
                <td>@order.Price</td>
                <td>@order.Qty</td>
            </tr>

        }
    </table>
</div>
